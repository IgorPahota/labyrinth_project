<div class="bottom-container">
    <div id="current_date_time_block2"></div>
    <span style="padding-left: 15px">Время эксперимента: </span><span id="countTime"></span>
    <br/>
    <button id="start-experiment" class="powerBtn btn" onclick="startCount();startExperiment()">Начать эксперимент</button>
    <br/>
    <button class="powerBtn btn" onclick="stopCount()">Остановить эксперимент</button>
    <br/>
    <button class="powerBtn btn" onclick="savePattern()">Сохранить шаблон</button>
    <br/>
    <label>
        <input class="actions" value="">
    </label>
    <div class="constBtns">
        <button class="constBtn btn" onclick="changeColor(this, 'lightgrey')">ПОЛЕ</button>
        <button class="constBtn btn" onclick="changeColor(this, 'darkblue')">СТЕНА</button>
        <button class="constBtn btn" onclick="changeColor(this, 'violet')">ЛОЖНАЯ КОРМУШКА</button>
        <button class="constBtn btn" onclick="changeColor(this, 'orange')">КОРМУШКА</button>
        <button class="constBtn btn" onclick="changeColor(this, 'green')">ВХОД</button>
        <button class="constBtn btn" onclick="changeColor(this, 'lightblue')">ВЫХОД</button>
        <button class="constBtn btn" onclick="positionStart()">НАЧАЛЬНАЯ ПОЗИЦИЯ</button>
    </div>
</div>
