    <div class="keyboard-container">
        <div class="keyboard" onselectstart="return false">
            <div class="leftBtns">
                <div class="smallBtnFld">
                    <div class="btnRow">
                        <div class="smallBtn" style="border-top-left-radius: 5px"><span class="ltb">t</span><span class="rbb">?</span></div>
                        <div class="smallBtn"><span class="ltb">h</span><span class="rbb">_</span></div>
                        <div class="smallBtn"><span class="ltb">o</span><span class="rbb">i</span></div>
                    </div>
                    <div class="btnRow">
                        <div class="smallBtn"><span class="ltb">j</span><span class="rbb">q</span></div>
                        <div class="smallBtn"><span class="ltb">╎</span><span class="rbb">b</span></div>
                        <div class="smallBtn"><span class="ltb">s</span><span class="rbb">l</span></div>
                    </div>
                    <div class="btnRow">
                        <div class="smallBtn"><span class="ltb">e</span><span class="rbb">x</span></div>
                        <div class="smallBtn"><span class="ltb">w</span><span class="rbb">z</span></div>
                        <div class="smallBtn"><span class="ltb">r</span><span class="rbb">m</span></div>
                    </div>
                </div>
                <div class="btnRow">
                    <div class="wideBtn" style="border-bottom-left-radius: 5px"><div class="wltb">y</div><div class="wrbb">d</div></div>
                    <div class="smallBtn"><span class="ltb">k</span><span class="rbb">g</span></div>
                </div>
            </div>
            <div class="longBtn" style="border-bottom-right-radius: 5px; border-top-right-radius: 5px"><div class="lltb">++</div><div class="lrbb">a</div></div>
        </div>
        <div id="deleteButton">УДАЛИТЬ</div>
